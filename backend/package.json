{
	"name": "backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prisma:dev:deploy": "prisma migrate deploy",
		"prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
		"prisma:generate": "prisma generate",
		"db:dev:rm": "docker compose rm dev-db -s -f -v",
		"db:dev:up": "docker compose up dev-db -d",
		"db:dev:restart:win": "npx prisma migrate dev && npm run db:dev:rm && npm run db:dev:up && timeout /t 1 && npm run prisma:dev:deploy",
		"db:dev:restart:unix": "npm run db:dev:rm && npm run db:dev:up && sleep 2 && npx prisma migrate dev && npm run prisma:dev:deploy",
		"db:test:rm": "docker compose rm test-db -s -f -v",
		"db:test:up": "docker compose up test-db -d",
		"db:test:restart": "npm run db:test:rm && npm run db:test:up && sleep 1 && npm run prisma:test:deploy",
		"build": "nest build",
		"setup:build:win": "npm run db:dev:restart:win && npm run build",
		"setup:build:unix": "npm run db:dev:restart:unix && npm run build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start --watch",
		"on": "lt --port 3000",
		"start:dev:unix": "npm run setup:build:unix && nest start --watch",
		"start:dev:win": "npm run setup:build:win && nest start --watch",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:debug:profile": "node --prof dist/main.js",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"lint:format": "npm run lint && npm run format",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "npm run db:test:restart",
		"test:e2e": "dotenv -e .env.test -- jest --verbose --watch --runInBand --no-cache --config ./test/jest-e2e.json",
		"test:prisma:studio": "dotenv -e .env.test -- npx prisma studio"
	},
	"dependencies": {
		"@nestjs/common": "^9.4.2",
		"@nestjs/config": "^2.3.1",
		"@nestjs/core": "^9.4.2",
		"@nestjs/jwt": "^10.0.3",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "^9.0.0",
		"@nestjs/platform-socket.io": "^9.4.0",
		"@prisma/client": "^5.0.0",
		"@socket.io/redis-adapter": "^8.2.0",
		"argon2": "^0.30.3",
		"body-parser": "^1.20.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
		"dotenv": "^16.0.3",
		"dotenv-cli": "^7.2.1",
		"express-session": "^1.17.3",
		"otplib": "^12.0.1",
		"passport": "^0.6.0",
		"passport-42": "^1.2.6",
		"passport-jwt": "^4.0.1",
		"qrcode": "^1.5.3",
		"redis": "^4.6.6",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.2.0",
		"socket.io": "^4.6.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.0.0",
		"@nestjs/schematics": "^9.0.0",
		"@nestjs/testing": "^9.0.0",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/express-session": "^1.17.7",
		"@types/jest": "29.5.0",
		"@types/node": "18.15.11",
		"@types/passport-jwt": "^3.0.8",
		"@types/qrcode": "^1.5.0",
		"@types/supertest": "^2.0.11",
		"@types/totp-generator": "^0.0.5",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"bcrypt": "^5.1.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "29.5.0",
		"pactum": "^3.4.0",
		"prettier": "^2.8.8",
		"prisma": "^5.0.0",
		"socket.io-client": "^4.6.2",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"totp-generator": "^0.0.14",
		"ts-jest": "29.0.5",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "4.2.0",
		"typescript": "^4.7.4"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
